<h1>Gazeteci Listesi</h1>

@Component.InvokeAsync("WorkChart") 

<div class="row row-cols-md-3">

    @foreach (Journalist item in Model)
    {
        <div class="col">
            <partial name="~/Views/Journalist/Partials/_JournalistInfoPartial.cshtml" model="@item" />
        </div>
    }

</div>